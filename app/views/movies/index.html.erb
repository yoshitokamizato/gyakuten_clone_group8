<div class="container">
  <% @movies.each do |movie| %>
    <div class="text-center my-4">
      <div class="col-xs-12">
        <%= "#{movie.name}" %><br>
        <iframe src=<%= "#{movie.url}" %> width="560" height="315" frameborder="0" style="border:0" allowfullscreen></iframe>
      </div>
    </div>
  <% end %>
  <%= paginate @movies %>
</div>
